# Google Maps API (geocoding + Street View)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Google AI Studio API (for Gemini 2.0 Flash vision scoring)
# You can use the same key as GOOGLE_MAPS_API_KEY or create a separate one
GOOGLE_API_KEY=your_google_ai_studio_api_key_here

# Anthropic API (legacy - now using Gemini 2.0 Flash)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Flask
FLASK_ENV=development
PORT=5000

# Stripe (for payments)
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here
